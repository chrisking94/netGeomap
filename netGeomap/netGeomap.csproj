<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Chris.King</Authors>
    <Description>A Geomap API .NET library.</Description>
    <Copyright>Chris</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/chrisking94/netGeomap/tree/master/netGeomap</PackageProjectUrl>
    <PackageIcon>netGeomap.png</PackageIcon>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/chrisking94/netGeomap.git</RepositoryUrl>
    <PackageTags>Geomap API txt</PackageTags>
    <PackageReleaseNotes>1. Update to .NET Core 3.0</PackageReleaseNotes>
    <Version>1.2.0</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <WarningLevel>1</WarningLevel>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\netGeomap.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
